list = []
el = []
ol = []
n = int(input("Enter the size of array:"))
print("Enter list elements:")
for i in range(n):
    x = int(input())
    list.append(x)
for i in range(n):
    if list[i]%2==0:
        y = list[i]
        el.append(y)
    else:
        z = list[i]
        ol.append(z)
print("The even numbers in given list are:",el)
print("The odd numbers in given list are:",ol)
